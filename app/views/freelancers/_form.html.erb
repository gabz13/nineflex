<div class="form-color">
  <%= simple_form_for(@freelancer) do |f| %>
      <%= f.input :first_name, input_html: { class: "animal-input"} %>
      <%= f.input :last_name, input_html: { class: "animal-input"} %>
      <%= f.input :email, input_html: { class: "animal-input"} %>


      <%= f.input :description, input_html: { class: "animal-input"} %>
      <%= f.submit class: "btn btn-info btn-block form-buttom" %>


  <% end %>
</div>

