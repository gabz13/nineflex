<div class="card-animal mt-3">
          <div class="card-image">
            <%#= cl_image_tag freelancer.photo, height: 200, crop: :fit%>
          </div>
          <div class="card-info">
            <h3><%#= @freelancer.first_name %></h3>
            <p><%= @freelancer %></p>
            <%#= cl_image_tag @freelancer.photo, height: 300, width: 400, crop: :fill %>
            <div class="card-button">
              <%= link_to "Book", freelancers_path(@freelancer), class: "btn btn-outline-info btn-sm" %>
            </div>
          </div>
          <!-- <img src="https://images.pexels.com/photos/1232152/pexels-photo-1232152.jpeg?auto=compress&cs=tinysrgb&dpr=2&w=500"> -->
        </div>
